<h1><%= @team.name %> Projects </h1>
<h2> 
	<% @team.projects.each do |project| %>
	<li><%= link_to project.title, project_url(project.id) %> - 
		<%= project.description %></li>
		<br>
	<% end %>
</h2>

<br>
<center><h3><%= @team.name %> team members </h3></center>
<h2> 
	<% @team.users.each do |user| %>
	<li><%= link_to user.name, user_url(user.id) %></li>
	<br>
	<% end %>
</h2>

<center>
	<%= link_to "Edit team", edit_team_url(@team.id), class: "btn" %>
	<%= link_to "Delete team", team_url(@team.id), method: :delete, class: "btn" %>
	<%= link_to "Add project", new_team_project_url(@team.id), class: "btn" %>
</center> 